    "function find_vasp_files(file)\n",
    "    if isfile(file)\n",
    "    else\n",
    "            return (\"$file not exist\")\n",
    "    end\n",
    "end\n",
    "\n",
    "function get_bandInfo(inFile=\"OUTCAR\")\n",
    "    find_vasp_files(inFile)\n",
    "end\n",
    "    \n",
    "function main()\n",
    "    get_bandInfo()\n",
    "end\n",
    "\n",
    "main()"
